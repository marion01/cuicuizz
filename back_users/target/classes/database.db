CREATE TABLE "User" ("Id" INTEGER PRIMARY KEY NOT NULL UNIQUE, "Pseudo" TEXT NOT NULL UNIQUE, "Password" TEXT NOT NULL, "LastActionDate" DATE);
CREATE TABLE "Score" ("Id" INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL UNIQUE, "UserId" TEXT NOT NULL, "Mode" TEXT NOT NULL, "Theme" TEXT NOT NULL, "NbQuestions" TEXT NOT NULL, "Value" TEXT NOT NULL);